{
  "remote.SSH.remotePlatform": {
    "mldl_R": "linux",
    "mldl01": "linux",
    "NAS": "linux",
    "DH_Nas": "linux"
    
  },
  "explorer.confirmDragAndDrop": false,
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one",
    "editor.tabSize": 4,
    "editor.indentSize": 4
  },
  "editor.codeActionsOnSave": {},
  "editor.snippetSuggestions": "top",
  "emmet.showSuggestionsAsSnippets": true,
  "jupyter.widgetScriptSources": [
    "jsdelivr.com",
    "unpkg.com"
  ],
  "[html]": {
    "editor.defaultFormatter": "mohd-akram.vscode-html-format"
  },
  "security.allowedUNCHosts": [
    "Modern_park"
  ],
  "window.zoomLevel": 1,
  "update.mode": "start",
  "workbench.preferredLightColorTheme": "Abyss",
  "workbench.editorAssociations": {
    "*.csv": "gc-excelviewer-csv-editor"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "markdownlint.run": "onType",
  "markdownlint.config": {
    "MD001": false,
    "MD007": {
      "indent": 4
    },
    "MD022": false,
    "MD032": false,
    "MD041": false,
    "no-hard-tabs": false
  },
  "editor.minimap.size": "fill",
  "editor.minimap.renderCharacters": false,
  "terminal.integrated.fontFamily": "D2CodingLigature Nerd Font Propo",
  "terminal.integrated.fontSize": 15,
  "emmet.includeLanguages": {
    "python": "python"
  },
  "editor.minimap.enabled": false,
  "r.source.encoding": "cp949",
  "editor.accessibilitySupport": "off",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.enableImages": true,
  "workbench.colorTheme": "Catppuccin Mocha",
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "debug.console.acceptSuggestionOnEnter": "on",
  "files.associations": {
    "*.rmd": "markdown"
  },
  "git.openRepositoryInParentFolders": "never",
  "workbench.sideBar.location": "right",
  "bookmarks.label.suggestion": "suggestWhenSelected",
  "notebook.formatOnCellExecution": true,
  "notebook.stickyScroll.enabled": true,
  "editor.stickyScroll.enabled": true,
  "workbench.tree.enableStickyScroll": true,
  "terminal.integrated.stickyScroll.enabled": true,
  "emmet.preferences": {},
  "settingsSync.ignoredSettings": [
    "notebook.formatOnSave.enabled",
    "jupyter.enableExtendedPythonKernelCompletions",
    "editor.formatOnSave",
    "editor.formatOnType",
    "-python.interpreter.infoVisibility"
  ],
  "jupyter.completionTriggerCharacters": {
    "bash": [
      "$",
      "{"
    ],
    "java": [
      ".",
      "@",
      "#"
    ],
    "javascript": [
      ".",
      "\"",
      "'",
      "/",
      "@"
    ],
    "python": [
      ".",
      "%"
    ],
    "julia": [
      "."
    ],
    "r": [
      "[",
      "(",
      ",",
      "$",
      "@",
      "\"",
      "'"
    ],
    "rust": [
      ".",
      ":"
    ],
    "scala": [
      "."
    ],
    "typescript": [
      ".",
      "\"",
      "'",
      "/",
      "@",
      "<"
    ]
  },
  "python.analysis.inlayHints.callArgumentNames": "partial",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "jupyter.enableExtendedPythonKernelCompletions": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "editor.formatOnSave": true,
  "notebook.formatOnSave.enabled": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorStyleInactive": "line",
  "terminal.integrated.detectLocale": "on",
  "terminal.integrated.enableVisualBell": true,
  "editor.tabCompletion": "on",
  "editor.unicodeHighlight.nonBasicASCII": false,
  "python.globalModuleInstallation": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.typeCheckingMode": "basic",
  "pythonIndent.trimLinesWithOnlyWhitespace": true,
  "pythonIndent.useTabOnHangingIndent": true,
  "python.analysis.enablePytestSupport": false,
  "python.languageServer": "Jedi",
  "python.analysis.autoFormatStrings": true,
  "workbench.iconTheme": "catppuccin-mocha",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.openDebug": "neverOpen",
  "powershell.integratedConsole.focusConsoleOnExecute": false,
  "files.defaultLanguage": "powershell",
  "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?",
  "powershell.buttons.showPanelMovementButtons": true,
  "powershell.codeFolding.showLastLine": false,
  "powershell.sideBar.CommandExplorerVisibility": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "&",
      "color": "#c2426b",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "^",
      "color": "yellow",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
  ],
  "html.format.indentInnerHtml": true,
  "html.format.templating": true,
  "livePreview.debugOnExternalPreview": true,
  "explorer.confirmDelete": false,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jupyter.interactiveWindow.creationMode": "perFile",
  "editor.defaultFormatter": "ms-python.black-formatter",
  "editor.formatOnPaste": true,
  "notebook.defaultFormatter": "ms-python.black-formatter",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Ubuntu (WSL)": {
      "path": "C:\\Windows\\System32\\wsl.exe",
      "args": [
        "-d",
        "Ubuntu"
      ]
    }
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "window.commandCenter": 1
}