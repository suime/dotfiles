{
  "autoDocstring.generateDocstringOnEnter": false,
  "better-comments.highlightPlainText": true,
  "bookmarks.label.suggestion": "suggestWhenSelected",
  "catppuccin-noctis-icons.hidesExplorerArrows": false,
  "cline.preferredLanguage": "Korean - 한국어",
  "dataWrangler.debug": true,
  "dataWrangler.experiments.fastCsvParsing": true,
  "dataWrangler.startInEditModeForNotebookEntrypoints": true,
  "debug.console.acceptSuggestionOnEnter": "on",
  "debug.console.fontFamily": "D2CodingLigature Nerd Font Propo",
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.codeLensFontFamily": "D2CodingLigature Nerd Font Propo",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "D2CodingLigature Nerd Font Propo, Consolas, 'Courier New', monospace",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.inlayHints.fontFamily": "D2CodingLigature Nerd Font Propo",
  "editor.inlineSuggest.fontFamily": "D2CodingLigature Nerd Font Propo",
  "editor.maxTokenizationLineLength": 40000,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.size": "fill",
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.tabCompletion": "onlySnippets",
  "editor.unicodeHighlight.nonBasicASCII": false,
  "emmet.showSuggestionsAsSnippets": true,
  "explorer.confirmDragAndDrop": false,
  "git.openRepositoryInParentFolders": "never",
  "jupyter.enableExtendedPythonKernelCompletions": true,
  "jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
  "notebook.formatOnCellExecution": false,
  "notebook.formatOnSave.enabled": true,
  "notebook.output.fontFamily": "D2CodingLigature Nerd Font Propo",
  "notebook.stickyScroll.enabled": true,
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.cacheLSPData": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.enablePytestSupport": false,
  "python.analysis.inlayHints.callArgumentNames": "partial",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.globalModuleInstallation": true,
  "python.languageServer": "Pylance",
  "redhat.telemetry.enabled": false,
  "ruff.enable": true,
  "ruff.nativeServer": "auto",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.external.osxExec": "iterm2.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorStyleInactive": "line",
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.detectLocale": "on",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.fontFamily": "D2CodingLigature Nerd Font Propo",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.stickyScroll.enabled": true,
  "update.mode": "start",
  "vbaFormatter.breakOnSeperator": true,
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Catppuccin Noctis Frappé",
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "catppuccin noctis icons",
  "workbench.preferredLightColorTheme": "Abyss",
  "workbench.sideBar.location": "right",
  "workbench.tree.enableStickyScroll": true,
  "remote.SSH.remotePlatform": {
    "NAS": "linux",
    "DH_Nas": "linux",
    "dh_nas": "linux"
  },
  "[markdown]": {
    "editor.tabSize": 4,
    "editor.indentSize": 4
  },
  "[html]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "workbench.editorAssociations": {
    "*.csv": "gc-excelviewer-csv-editor",
    "*.frx": "default"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    }
  },
  "emmet.includeLanguages": {
    "python": "python"
  },
  "files.associations": {
    "*.rmd": "markdown",
    "*.dcm": "vba"
  },
  "jupyter.completionTriggerCharacters": {
    "bash": ["$", "{"],
    "java": [".", "@", "#"],
    "javascript": [".", "\"", "'", "/", "@"],
    "python": [".", "%"],
    "julia": ["."],
    "r": ["[", "(", ",", "$", "@", "\"", "'"],
    "rust": [".", ":"],
    "scala": ["."],
    "typescript": [".", "\"", "'", "/", "@", "<"]
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false,
      "isRegex": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false,
      "isRegex": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false,
      "isRegex": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false,
      "isRegex": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false,
      "isRegex": false
    },
    {
      "tag": "-{5,}",
      "color": "#000000",
      "backgroundColor": "#fff",
      "bold": true,
      "isRegex": true,
      "isWholeLine": true
    }
  ],
  "frameIndentRainbow.colors": [
    "hsla(312, 70%, 50%, 0.2)",
    "hsla(250, 70%, 50%, 0.2)",
    "hsla(191, 70%, 50%, 0.2)",
    "hsla(20, 70%, 50%, 0.2)"
  ],
  "frameIndentRainbow.updateDelay": 1000,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[vb]": {
    "editor.defaultFormatter": "threatcon.vba-formatter"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}
