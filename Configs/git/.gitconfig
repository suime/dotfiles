[user]
name = DHP
email = suime@github.com
[init]
defaultBranch = main
[core]
excludesfile = ~/.gitignore
[init]
defaultBranch = main
templatedir = ~/.git_template
[push]
default = current
[color]
ui = auto
[alias]
s = status
aa = add --all
ap = add --patch
branches = for-each-ref --sort=-committerdate --format='%(color:blue)%(authordate:relative)	%(color:red)%(authorname)	%(color:white)%(color:bold)%(refname:short)' refs/remotes
ci = commit -v
cm = commit -m
co = checkout
p = push
pf = push --force-with-lease
[merge]
ff = only
[fetch]
prune = true
[rebase]
autosquash = true
[diff]
colorMoved = zebra
[http]
sslVerify = false
