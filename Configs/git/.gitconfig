name = DHP
	email = qkqxk600@gmail.com
[init]
defaultBranch = main
[core]
excludesfile = ~/.gitignore
	autocrlf = false
safecrlf = true
[init]
defaultBranch = main
templatedir = ~/.git_template
[push]
default = current
[color]
ui = auto
[alias]
s = status
aa = add --all
ap = add --patch
branches = for-each-ref --sort=-committerdate --format='%(color:blue)%(authordate:relative)	%(color:red)%(authorname)	%(color:white)%(color:bold)%(refname:short)' refs/remotes
ci = commit -v
cm = commit -m
co = checkout
pl = pull
ps = push
pf = push --force-with-lease
[merge]
ff = only
[fetch]
prune = true
[rebase]
autosquash = true
[diff]
colorMoved = zebra
[http]
	sslVerify = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[safe]
	directory = G:/dotfiles
